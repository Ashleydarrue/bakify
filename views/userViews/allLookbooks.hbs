 <section class="allCards">
      <div class="row">
        {{#each lookbooks}}        
          <div class="col s12 m6">
            <div class="card">
              <div class="card-image">
                <img src={{this.imgPath}}>
                </div>
                <div class="card-content">
                <p>This Cake was requested by: {{this.owner}}</p>
                <p>{{this.comment}}</p>
                </div>
                <div class="card-action">
                <button class="dlt">Add</button>
            </div>
           </div>
         </div>   
        {{/each}}
    </div>
</section>


























