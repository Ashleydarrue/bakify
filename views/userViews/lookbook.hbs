 <section class="lookbookContent">
   <p> Add Images to your LookBook</p>
 </section>
     
     <section class="addimg">
        <form action="/look/addimage" method="POST" enctype="multipart/form-data">
              <input type="file" name="thePic">

              <p>Comment:</p>
              <textarea name="comment" id="comment" cols="5" rows="5" style="margin: 0px; width: 161px; height: 41px;"></textarea>

              <button class="upload"><i class="fas fa-upload"></i>Submit</button>
        </form>
     </section>

     <section class"userLookbook">
       {{#each image }}

        <img src="{{this.imagePath}}"/>
        <p>{{this.comment}}</p>
       {{/each}}
     </section>







